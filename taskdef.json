{
  "executionRoleArn": "arn:aws:iam::761280559302:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "springboot-gradle-docker-aws",
      "image": "761280559302.dkr.ecr.us-east-2.amazonaws.com/springboot-gradle-docker-aws:latest",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "protocol": "tcp",
          "containerPort": 80
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "family": "springboot-gradle-docker-aws"
}